{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {        
      "@/*":["src/*"],      
      "@react/*":["src/components/react/*"],
      "@js/*":["src/components/js/*"],
      "@astro/*":["src/components/astro/*"],
      "@styles/*":["src/styles/*"],
      "@layouts/*":["src/layouts/*"],
      "@img/*":["src/img/*"],
    }
  }
}